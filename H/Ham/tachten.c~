#include<stdio.h>
#include<string.h>
char tachten(char a[],char b[])
{ int i,n,k;
  printf("Nhap ten day du : ");gets(a);
  while(strstr(a," ")==NULL)
    {
      printf("Ban phai nhap day du , hay nhap lai : ");gets(a);
  }
  n=strlen(a);
  k=strlen(b);j=0;
  for(i=n-1;i>=0;i--)
    {
      if(a[i]!=' ') {b[j]=a[i];j++;} else if(a[i]==' ') break;
    }
}
